					"listen": "test",
					"script": {
						"exec": [
							"pm.test(\"Register Patient and Set Environment Variable\", function () {",
							"    pm.response.to.have.status(201);",
							"    const responseJson = pm.response.json();",
							"    const patientEmail = responseJson.email;",
							"    console.log(\"Set Environment Variable: \", patientEmail);",
							"    pm.environment.set(\"patientEmail\", patientEmail);",
							"});"
